
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option channel '11'
	option hwmode '11ng'
	option htmode 'HT20'
	option disabled '0'

config wifi-iface 'wlan0'
	option 'device'     'radio0'
	option 'network'    'wlan'
	option 'mode'       'sta'
	option 'ssid'       'GoldorakIOT'
	option 'encryption' 'psk2'
	option 'key'        '???'
